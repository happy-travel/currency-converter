<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFrameworks>netcoreapp3.1;net5.0</TargetFrameworks>
    <Nullable>enable</Nullable>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <Company>Happytravel.com</Company>
    <Authors>Kirill Taran</Authors>
    <PackageProjectUrl>https://github.com/happy-travel/currency-converter</PackageProjectUrl>
    <Description>The currency conversion library</Description>
    <RepositoryUrl>https://github.com/happy-travel/currency-converter.git</RepositoryUrl>
    <PackageTags>currency conversion currency-conversion</PackageTags>
    <Version>1.0.0.1</Version>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="HappyTravel.Money" Version="1.3.1" />
    <PackageReference Include="Microsoft.Extensions.DependencyInjection.Abstractions" Version="5.0.0" />
  </ItemGroup>
</Project>